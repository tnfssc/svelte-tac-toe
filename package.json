{
  "name": "svelte-tac-toe",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-web": "cross-env NODE_ENV=development vite",
    "build-web": "cross-env NODE_ENV=production vite build",
    "start-web": "vite preview",
    "dev-api": "cross-env NODE_ENV=development nodemon",
    "build-api": "cross-env NODE_ENV=production node scripts/build-server.js",
    "start-api": "cross-env NODE_ENV=production node dist/api/server.js",
    "dev": "concurrently --kill-others \"yarn:dev-*\"",
    "build": "yarn build-web && yarn build-api",
    "start": "concurrently --kill-others \"yarn:start-*\"",
    "prepare": "husky install",
    "pretty": "prettier --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.7",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/node": "^1.10.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "esbuild": "^0.11.18",
    "eslint": "^7.25.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "fastify": "^3.15.1",
    "fastify-cors": "^6.0.0",
    "fastify-helmet": "^5.3.1",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.32.12",
    "svelte": "^3.37.0",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.7.2",
    "typescript": "^4.2.4",
    "vite": "^2.2.3"
  },
  "prettier": {
    "svelteSortOrder": "options-scripts-markup-styles",
    "svelteStrictMode": true,
    "svelteBracketNewLine": false,
    "svelteAllowShorthand": false,
    "svelteIndentScriptAndStyle": true
  },
  "dependencies": {}
}
